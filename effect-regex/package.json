{
  "name": "@template/cli",
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "license": "MIT",
  "description": "A basic Effect CLI application",
  "repository": {
    "type": "git",
    "url": "<PLACEHOLDER>"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "build": "tsup && pnpm copy-package-json",
    "build:ts": "tsup",
    "build:mcp": "tsup src/mcp/server.ts --format cjs --out-dir dist --clean",
    "clean": "rimraf dist/*",
    "check": "tsc -b tsconfig.json",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "copy-package-json": "tsx scripts/copy-package-json.ts",
    "changeset-version": "changeset version && node scripts/version.mjs",
    "changeset-publish": "pnpm build && TEST_DIST= pnpm vitest && changeset publish",
    "mcp:dev": "tsx src/mcp/server.ts"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.8",
    "@effect/cli": "^0.69.0",
    "@effect/language-service": "latest",
    "@effect/platform": "^0.90.3",
    "@effect/platform-node": "^0.96.0",
    "@effect/vitest": "^0.25.1",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@types/node": "^22.5.2",
    "effect": "^3.17.7",
    "tsup": "^8.2.4",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^3.2.0"
  },
  "patchedDependencies": {
    "@changesets/get-github-info@0.6.0": "patches/@changesets__get-github-info@0.6.0.patch"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0"
  }
}
